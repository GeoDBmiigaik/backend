[metadata]
name = GeoDB
description = Backend for the "GeoDB" project

[options]
packages = find_namespace:
include_package_data = True
zip_safe = False

install_requires =
    uvicorn
    starlette

testing =
    setuptools
    pytest

[options.packages.find]
where = .
exclude =
    tests

[options.entry_points]
console_scripts =
    website = geodb.website.test:main